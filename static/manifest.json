{
  "manifest_version": 3,
  "name": "extensionRep",
  "version": "1.0.0",
  "description": "A Chrome extension that adds users to a firebase database.",
  "permissions": ["storage", "scripting", "identity"],
  "host_permissions": ["https://your-auth-server.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}